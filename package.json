{
  "name": "react-ultimate-pagination-bootstrap-3",
  "version": "1.0.2",
  "description": "React.js pagination component with Bootstrap 3 theme",
  "main": "lib/react-ultimate-pagination-bootstrap-3.js",
  "keywords": [
    "react",
    "react.js",
    "component",
    "react-component",
    "pagination",
    "paginator",
    "pager",
    "bootstrap"
  ],
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "prebuild": "rm -rf dist lib",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel src -d lib",
    "build:umd": "webpack --output-filename react-ultimate-pagination-bootstrap-3.js",
    "build:umd.min": "webpack --output-filename react-ultimate-pagination-bootstrap-3.min.js -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ultimate-pagination/react-ultimate-pagination-bootstrap-3.git"
  },
  "author": "Dmytro Yarmak <dmytroyarmak@gmail.com> (http://dmytroyarmak.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ultimate-pagination/react-ultimate-pagination-bootstrap-3/issues"
  },
  "homepage": "https://github.com/ultimate-pagination/react-ultimate-pagination-bootstrap-3#readme",
  "dependencies": {
    "react-ultimate-pagination": "^1.0.2"
  },
  "peerDependencies": {
    "bootstrap": "3.x"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "npm-run-all": "^1.8.0",
    "webpack": "^1.13.0"
  }
}
